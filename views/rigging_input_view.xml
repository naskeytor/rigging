<odoo>
    <data>

        <record id="rigging_input_form" model="ir.ui.view">
            <field name="name">rigging.input.form</field>
            <field name="model">rigging.input</field>
            <field name="arch" type="xml">
                <form string="Input Form">
                    <group>
                        <field name='aad_jumps_input' string="Test"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="rigging_input_action" model="ir.actions.act_window">
            <field name="name">Input Aad Jumps</field>
            <field name="res_model">rigging.input</field>
            <field name="view_mode">tree,form</field>
            <field name="target">new</field>
        </record>

    </data>
</odoo>